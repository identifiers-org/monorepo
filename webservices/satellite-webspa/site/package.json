{
  "name": "cloud-satellite-web-frontend",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "version": "2.3.0",
  "description": "Identifiers.org satellite web frontend",
  "scripts": {
    "start": "rm -Rf .parcel-cache; PARCEL_WORKERS=0 NODE_ENV=development parcel src/index.html --port 9091 --hmr-port 50001",
    "build": "rm -Rf .parcel-cache; PARCEL_WORKERS=0 NODE_ENV=production parcel build src/index.html --dist-dir dist",
    "precommit": "lint-staged",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "test:eslint": "eslint src/ --ext .js --ext .jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javfg/cloud-satellite-web-frontend.git"
  },
  "author": "Javier Ferrer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javfg/cloud-satellite-web-frontend/issues"
  },
  "homepage": "https://github.com/javfg/cloud-satellite-web-frontend#readme",
  "dependencies": {
    "@datapunt/matomo-tracker-react": "^0.5.1",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^4.6.2",
    "ebi-framework": "^1.4.0-rc.1",
    "EBI-Icon-fonts": "^1.3.0",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.4.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.0",
    "sweetalert2": "^11.4.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.0",
    "@babel/plugin-transform-class-properties": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.0",
    "@parcel/transformer-sass": "^2.12.0",
    "autoprefixer": "^9.6.1",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.14.3",
    "parcel": "^2.12.0",
    "postcss": "^8.4.19",
    "process": "^0.11.10",
    "sass": "^1.79.0"
  },
  "engines": {
    "node": ">= 10"
  }
}
